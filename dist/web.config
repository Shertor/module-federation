<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <directoryBrowse enabled="false" />
        <rewrite>
            <rules>
			    <rule name="Root Hit Redirect" stopProcessing="true">
					<match url="^$" />
					<action type="Rewrite" url="/shell/" />
				</rule>
				<rule name="Mfe Module Redirect" stopProcessing="true">
					<match url="mfe-module\/$" />
					<action type="Redirect" url="/shell/" />
				</rule>
				<rule name="Mfe Plugins Redirect" stopProcessing="true">
					<match url="mfe-plugins\/$" />
					<action type="Redirect" url="/shell/" />
				</rule>
            </rules>
        </rewrite>
        <httpRedirect enabled="false" destination="" httpResponseStatus="Permanent" />
    </system.webServer>
</configuration>
